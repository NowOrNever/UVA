#include<cstdio>

int main()
{
	int n,a,b,i,j;
	scanf("%d",&n);  
	while(n--){
	scanf("%d",&a);
	scanf("%d",&b);
	while(b--){
	for(i=1;i<=a;i++){
		for(j=0;j<i;j++)
		printf("%d",i);
		printf("\n");
		}
	for(i=a-1;i>0;i--){
		for(j=0;j<i;j++)
		printf("%d",i);
		printf("\n");
		}
	if(b)printf("\n");
	}
	if(n) printf("\n");
	}
	return 0;
}
